{
  "name": "@travelogue/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project tsconfig.app.json && tsc --noEmit --project tsconfig.node.json",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.71.5",
    "@travelogue/backend": "workspace:*",
    "@trpc/client": "^11.0.2",
    "@trpc/react-query": "^11.0.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.1",
    "reset-css": "^5.0.2",
    "sass": "^1.87.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "sass-migrator": "^2.3.1",
    "stylelint": "^16.19.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
